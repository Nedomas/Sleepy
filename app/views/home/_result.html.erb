<p>
<% if @wake_result %>
<em>Try to wake-up at times:</em><br>
	<% @wake_result.each do |f| %>
	<span style="color:<%= cycle("#6EA25A", "#6EA25A", "#E89B34", "#E89B34", "#B24A3C", "#B24A3C", :name => "colors") %>">
		<%= f.strftime("%H:%M") %> ..
		</span>
	<% end %>
<% else %>
<em>Go to sleep at:</em><br>
	<% @sleep_result.each do |f| %>
		<span style="color:<%= cycle("#6EA25A", "#6EA25A", "#E89B34", "#E89B34", "#B24A3C", "#B24A3C", :name => "colors") -%>">
		<%= f.strftime("%H:%M") %> ..
		</span>
	<% end %>
<% end %>
</p>
<% reset_cycle("colors") %>
<%= button_to 'Back', home_clean_path, :remote => :true, :class => "btn btn-large btn-success"%>